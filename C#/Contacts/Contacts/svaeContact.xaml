<Window x:Class="Contacts.svaeContact"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Contacts"
        mc:Ignorable="d"
        Title="svaeContact" Height="450" Width="800">
    <Grid>
        <StackPanel Margin="10">
            <TextBox x:Name="IDBox" Margin="0,0,0,5" />
            <TextBox x:Name="nameBox" Margin="0,0,0,5" />
            <TextBox x:Name="emailBox" Margin="0,0,0,5" />
            <Button Content="Save" x:Name="saveButtom" Click="saveButtom_Click"/>
        </StackPanel>
    </Grid>
</Window>
